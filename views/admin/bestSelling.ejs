                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
              <h2 class="content-title">Best Selling Products</h2>
            </div>
            <div class="card mb-4">
                <div class="container">
                <div class="table-responsive">
                  <% if (bestSellingProducts != '') { %>
                    <table class="table">
                        <thead class="thead-light">
                      <tr class=>
                        <th>ID</th>
                        <th>Product Title</th>
                        <th>Total Quantity Sold</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% bestSellingProducts.forEach((product,index) => { %>
                      <tr>
                        <td><%= index+1 %></td>
                        <td><%= product.productTitle %></td>
                        <td><%= product.totalQuantity %></td>
                      </tr>
                      <% }); %>
                    </tbody>
                  </table>
                  <% }else{ %>
                  <h1 class="text-center text-secondary mt-5">No products</h1>
                  <% } %>
                </div>
              </div>
            </div>
          </section>
        <section class="content-main">
            <div class="content-header">
              <h2 class="content-title">Best Selling Brands</h2>
            </div>
            <div class="card mb-4">
                <div class="container">
                <div class="table-responsive">
                    <!-- <table class="table"> -->
                  <% if (bestSellingBrands != '') { %>
                    <table class="table">
                    <thead class="thead-light">
                      <tr>
                        <th>ID</th>
                        <th>Brand Title</th>
                        <th>Total Quantity Sold</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% bestSellingBrands.forEach((brand ,index) => { %>
                      <tr>
                        <td><%= index+1 %></td>
                        <td><%= brand.brandName %></td>
                        <td><%= brand .totalQuantity %></td>
                      </tr>
                      <% }); %>
                    </tbody>
                  </table>
                  <% }else{ %>
                  <h1 class="text-center text-secondary mt-5">No brands</h1>
                  <% } %>
                </div>
              </div>
            </div>
        </section>
  <section class="content-main">
    <div class="content-header">
      <h2 class="content-title">Best Selling Categories</h2>
    </div>
    <div class="card mb-4">
        <div class="container">
        <div class="table-responsive">
          <% if (bestSellingCategories != '') { %>
            <table class="table">
                <thead class="thead-light">
              <tr>
                <th>ID</th>
                <th>Category Title</th>
                <th>Total Quantity Sold</th>
              </tr>
            </thead>
            <tbody>
              <% bestSellingCategories.forEach((category ,index) => { %>
              <tr>
                <td><%= index+1 %></td>
                <td><%= category.category %></td>
                <td><%= category .totalQuantity %></td>
              </tr>
              <% }); %>
            </tbody>
          </table>
          <% }else{ %>
          <h1 class="text-center text-secondary mt-5">No categories</h1>
          <% } %>
        </div>
      </div>
    </div>
      </section>
